{
  "plugins": [
    "react"
  ],
  "env": {
    "node": true,
    "browser": true,
    "jquery": true
  },
  "ecmaFeatures": {
    "jsx": true
  },
  "rules": {
    "semi": 2,
    "no-bitwise": 2,
    "no-unused-vars": 2,
    "no-cond-assign": 2,
    "curly": 2,
    "eqeqeq": 2,
    "no-eq-null": 2,
    "no-eval": 2,
    "guard-for-in": 2,
    "wrap-iife": [2, "inside"],
    "no-use-before-define": 2,
    "no-loop-func": 2,
    "no-caller": 2,
    "no-empty-character-class": 2,
    "no-shadow": 2,
    "no-new-object": 2,
    "no-undef": 2,
    "quotes": [2, "single"],
    "jsx-quotes": [2, "prefer-double"],
    "indent": 2,
    "new-cap": 2,
    "no-empty": 2,
    "no-new": 2,
    "one-var": [2, "never"],
    "no-trailing-spaces": 2
  }
}
